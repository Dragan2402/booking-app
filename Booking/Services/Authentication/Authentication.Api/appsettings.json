{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ConnectionStrings": {
    "AuthenticationDatabase": "Server=(localdb)\\mssqllocaldb;Database=Booking.Authentication;Trusted_Connection=True;"
  },
  "JsonWebTokenConfiguration": {
    "DefaultIdentityTokenValidityInSeconds": 2592000,
    "DefaultRefreshTokenValidityInSeconds": 86400,
    "TokenIssuer": "BookingAuthentication"
  }
}
